NAME LANT

#include <eikon.rh>
#include <eikcore.rsg>

#include "PeriodicTable.hrh"

RESOURCE RSS_SIGNATURE {  }

/* Lanthanides */
RESOURCE TBUF r_lanthanum_general { buf="Name: lanthanum\nSymbol: La\nAtomic number: 57\nCategory: lanthanides\nGroup: 3\nPeriod: 6\nBlock: f\nAtomic weight: 138.90547 g/mol\nElectrons per shell: 2, 8, 18, 18, 9, 2\nCAS number: 7439-91-0"; }
RESOURCE TBUF r_lanthanum_physical { buf="Phase: solid\nDensity: 6.162 kg/l (near room temperature)\nMelting point: 1193 K (920°C, 1688°F)\nBoiling point: 3737 K (3464°C, 6267°F)"; }
RESOURCE TBUF r_lanthanum_atomic { buf="Crystal structure: hexagonal\nOxidation states: 3\nElectronegativity: 1.10 (Pauling scale)"; }
RESOURCE TBUF r_lanthanum_name { buf="Comes from: lanthano\nMeaning: a Greek word which means 'to lie hidden'"; }
RESOURCE TBUF r_lanthanum_other { buf="- is found in some rare-earth minerals, usually in\n  combination with cerium and other rare earth\n  elements\n- is malleable, ductile, and soft enough to be cut\n  with a knife\n- is one of the most reactive of the rare-earth\n  metals"; }
RESOURCE TBUF r_lanthanum_other2 { buf="- reacts directly with elemental carbon, nitrogen,\n  boron, selenium, silicon, phosphorus, sulfur, and\n  with halogens\n- oxidizes rapidly when exposed to air\n- has no known biological role"; }
RESOURCE TBUF r_lanthanum_other3 { buf="- uses include carbon lighting applications, metal\n  additive, petroleum cracking catalysts, glass\n  polishing compound etc."; }
RESOURCE TBUF r_cerium_general { buf="Name: cerium\nSymbol: Ce\nAtomic number: 58\nCategory: lanthanides\nGroup: n/a\nPeriod: 6\nBlock: f\nAtomic weight: 140.116 g/mol\nElectrons per shell: 2, 8, 18, 19, 9, 2\nCAS number: 7440-45-1"; }
RESOURCE TBUF r_cerium_physical { buf="Phase: solid\nDensity: 6.770 kg/l (near room temperature)\nMelting point: 1068 K (795°C, 1463°F)\nBoiling point: 3716 K (3443°C, 6229°F)"; }
RESOURCE TBUF r_cerium_atomic { buf="Crystal structure: face-centered cubic\nOxidation states: 3, 4\nElectronegativity: 1.12 (Pauling scale)"; }
RESOURCE TBUF r_cerium_name { buf="Comes from: Ceres\nMeaning: the name of a dwarf planet which was\n         discovered two years earlier than the\n         element"; }
RESOURCE TBUF r_cerium_other { buf="- is a silvery metal\n- resembles iron in color and luster, but is soft,\n  and both malleable and ductile\n- has the longest liquid range of any non-\n  radioactive element\n- oxidizes very readily at room temperature,\n  especially in moist air"; }
RESOURCE TBUF r_cerium_other2 { buf="- is likely to ignite if scratched\n- is never found in nature as the free element\n- is most often prepared via an ion exchange\n  process that uses monazite sands as its cerium\n  source"; }
RESOURCE TBUF r_cerium_other3 { buf="- is used in making aluminium alloys, in carbon-\n  arc lighting, in alloys that are used to make\n  permanent magnets"; }
RESOURCE TBUF r_praseodymium_general { buf="Name: praseodymium\nSymbol: Pr\nAtomic number: 59\nCategory: lanthanides\nGroup: n/a\nPeriod: 6\nBlock: f\nAtomic weight: 140.90765 g/mol\nElectrons per shell: 2, 8, 18, 21,8, 2\nCAS number: 7440-10-0"; }
RESOURCE TBUF r_praseodymium_physical { buf="Phase: solid\nDensity: 6.77 kg/l (near room temperature)\nMelting point: 1208 K (935°C, 1715°F)\nBoiling point: 3793 K (3520°C, 6368°F)"; }
RESOURCE TBUF r_praseodymium_atomic { buf="Crystal structure: hexagonal\nOxidation states: 3\nElectronegativity: 1.13 (Pauling scale)"; }
RESOURCE TBUF r_praseodymium_name { buf="Comes from: prasios and didymos\nMeaning: Greek words which mean green and twin\n         because it is used in glasses to give a\n         green color"; }
RESOURCE TBUF r_praseodymium_other { buf="- is a soft, silvery, malleable and ductile metal\n- develops a green oxide coating that spalls off\n  when exposed to air, exposing more metal to\n  oxidation - for this reason, praseodymium should\n  be stored under a light mineral oil or sealed in\n  glass"; }
RESOURCE TBUF r_praseodymium_other2 { buf="- is frequently misspelled as praseodynium\n- is available in small quantities in Earth’s crust\n  (9.5 ppm)\n- is found in the rare earth minerals monazite and\n  bastnasite, typically comprising about 5% of the\n  lanthanides contained therein"; }
RESOURCE TBUF r_praseodymium_other3 { buf="- like all rare earths, praseodymium is of low to\n  moderate toxicity\n- has no known biological role\n- is used in making aircraft engines, in carbon-arc\n  lighting, in glassware making to give them a\n  yellow color"; }
RESOURCE TBUF r_neodymium_general { buf="Name: neodymium\nSymbol: Nd\nAtomic number: 60\nCategory: lanthanides\nGroup: n/a\nPeriod: 6\nBlock: f\nAtomic weight: 144.242 g/mol\nElectrons per shell: 2, 8, 18, 22, 8, 2\nCAS number: 7440-00-8"; }
RESOURCE TBUF r_neodymium_physical { buf="Phase: solid\nDensity: 7.01 kg/l (near room temperature)\nMelting point: 1297 K (1024°C, 1875°F)\nBoiling point: 3347 K (3074°C, 5565°F)"; }
RESOURCE TBUF r_neodymium_atomic { buf="Crystal structure: hexagonal\nOxidation states: 3\nElectronegativity: 1.14 (Pauling scale)"; }
RESOURCE TBUF r_neodymium_name { buf="Comes from: neos and didymos\nMeaning: Greek words which mean new and twin"; }
RESOURCE TBUF r_neodymium_other { buf="- is present in Mischmetal to the extent of about\n  18%\n- has a bright, silvery metallic luster; however,\n  as one of the more reactive rare earth\n  (Lanthanide) metals, it quickly oxidizes in air\n- the oxide layer then falls off, which exposes the\n  metal to further oxidation"; }
RESOURCE TBUF r_neodymium_other2 { buf="- although a rare earth metal, neodymium is not\n  rare at all - it constitutes 38 ppm of the\n  Earth’s crust\n- neodymium magnets are the strongest permanent\n  magnets known\n- is frequently misspelled as neodynium"; }
RESOURCE TBUF r_neodymium_other3 { buf="- is never found in nature as the free element;\n  rather, it occurs in ores such as monazite\n  sand\n- neodymium metal dust is a combustion and\n  explosion hazard"; }
RESOURCE TBUF r_neodymium_other4 { buf="- commercially available magnets made from\n  neodymium are exceptionally strong, and can\n  attract each other from large distances - if not\n  handled carefully, they could come together\n  very quickly and forcefully, causing injuries"; }
RESOURCE TBUF r_promethium_general { buf="Name: promethium\nSymbol: Pm\nAtomic number: 61\nCategory: lanthanides\nGroup: n/a\nPeriod: 6\nBlock: f\nAtomic weight: [145] g/mol\nElectrons per shell: 2, 8, 18, 23, 8, 2\nCAS number: 7440-12-2"; }
RESOURCE TBUF r_promethium_physical { buf="Phase: solid\nDensity: 7.26 kg/l (near room temperature)\nMelting point: 1315 K (1042°C, 1908°F)\nBoiling point: 3273 K (3000°C, 5432°F)"; }
RESOURCE TBUF r_promethium_atomic { buf="Crystal structure: hexagonal\nOxidation states: 3\nElectronegativity: 1.13 (Pauling scale)"; }
RESOURCE TBUF r_promethium_name { buf="Comes from: Prometheus\nMeaning: the Titan, in Greek mythology, who stole\n         the fire from Mount Olympus and brought\n         it down to mankind - the name was\n         suggested by the discoverer's wife, who\n         felt that they were stealing fire from the\n         gods"; }
RESOURCE TBUF r_promethium_other { buf="- is notable for being the only other exclusively\n  radioactive element besides technetium which is\n  followed by chemical elements that have stable\n  isotopes\n- its longest lived isotope 145Pm is a soft beta\n  emitter with a half-life of 17.7 years"; }
RESOURCE TBUF r_promethium_other2 { buf="- does not emit gamma rays, but beta particles\n  impinging on elements of high atomic numbers can\n  generate X-rays\n- exists in two allotropic forms, and its chemistry\n  is similar to other lanthanides"; }
RESOURCE TBUF r_promethium_other3 { buf="- promethium salts luminesce in the dark with a\n  pale blue or greenish glow, due to their high\n  radioactivity\n- is used as a beta radiation source for thickness\n  gauges, in a nuclear battery and in the future,\n  portable X-ray sources"; }
RESOURCE TBUF r_promethium_other4 { buf="- can be formed in nature as a product of\n  spontaneous fission of uranium-238 and alpha\n  decay of europium-151\n- it was calculated that the equilibrium mass of\n  promethium in the Earth's crust is about 560 g"; }
RESOURCE TBUF r_promethium_other5 { buf="  due to uranium fission and about 12 g due to the\n  recently observed alpha decay of europium-151"; }
RESOURCE TBUF r_samarium_general { buf="Name: samarium\nSymbol: Sm\nAtomic number: 62\nCategory: lanthanides\nGroup: n/a\nPeriod: 6\nBlock: f\nAtomic weight: 150.36 g/mol\nElectrons per shell: 2, 8, 18, 24, 8, 2\nCAS number: 7440-12-2"; }
RESOURCE TBUF r_samarium_physical { buf="Phase: solid\nDensity: 7.52 kg/l (near room temperature)\nMelting point: 1345 K (1072°C, 1962°F)\nBoiling point: 2067 K (1794°C, 3261°F)"; }
RESOURCE TBUF r_samarium_atomic { buf="Crystal structure: rhombohedral\nOxidation states: 3\nElectronegativity: 1.17 (Pauling scale)"; }
RESOURCE TBUF r_samarium_name { buf="Comes from: samarskite\nMeaning: a mineral found in the Urals and in North\n         Carolina"; }
RESOURCE TBUF r_samarium_other { buf="- is a rare earth metal, with a bright silver\n  luster\n- is reasonably stable in air; it ignites in air at\n  150 °C\n- even with long-term storage under mineral oil,\n  samarium is gradually oxidized, with a grayish-\n  yellow powder of the oxide-hydroxide being\n  formed"; }
RESOURCE TBUF r_samarium_other2 { buf="- the metallic appearance of a sample can be\n  preserved by sealing it under an inert gas such\n  as argon\n- is used e.g. in carbon-air lighting, as a neutron\n  absorber, for alloys, for headphone magnets"; }
RESOURCE TBUF r_samarium_other3 { buf="- is never found free in nature, but, like other\n  rare earth elements, is contained in many\n  minerals, including monazite, bastnäsite and\n  samarskite"; }
RESOURCE TBUF r_samarium_other4 { buf="- is of low to moderate toxicity, although their\n  toxicity has not been investigated in detail\n- is composed of four stable isotopes, and three\n  extremely long-lived radioisotopes"; }
RESOURCE TBUF r_europium_general { buf="Name: europium\nSymbol: Eu\nAtomic number: 63\nCategory: lanthanides\nGroup: n/a\nPeriod: 6\nBlock: f\nAtomic weight: 151.964 g/mol\nElectrons per shell: 2, 8, 18, 25, 8, 2\nCAS number: 7440-12-2"; }
RESOURCE TBUF r_europium_physical { buf="Phase: solid\nDensity: 5.264 kg/l (near room temperature)\nMelting point: 1099 K (826 °C, 1519°F)\nBoiling point: 1802 K (1529°C, 2784°F)"; }
RESOURCE TBUF r_europium_atomic { buf="Crystal structure: simple cubic (body centered)\nOxidation states: 3, 2\nElectronegativity: 1.2 (Pauling scale)"; }
RESOURCE TBUF r_europium_name { buf="Comes from: Europe\nMeaning: the continent"; }
RESOURCE TBUF r_europium_other { buf="- is the most reactive of the rare earth elements;\n  it rapidly oxidizes in air, and resembles calcium\n  in its reaction with water\n- ignites in air at about 150 °C to 180 °C\n- is about as hard as lead and quite ductile"; }
RESOURCE TBUF r_europium_other2 { buf="- is a metal and becomes a superconductor under\n  pressure 80 GPa at temperature 1.8 K\n- has been used to dope some types of glass to make\n  lasers, as well as for screening for Down\n  syndrome and some other genetic diseases"; }
RESOURCE TBUF r_europium_other3 { buf="- due to its amazing ability to absorb neutrons, it\n  is also being studied for use in nuclear\n  reactors\n- europium oxide (Eu2O3) is widely used as a red\n  phosphor in television sets and fluorescent\n  lamps, and as an activator for yttrium-based\n  phosphors"; }
RESOURCE TBUF r_europium_other4 { buf="- is never found in nature as a free element;\n  however, there are many minerals containing\n  europium, with the most important sources being\n  bastnäsite and monazite\n- the metal dust presents a fire and explosion\n  hazard\n- has no known biological role"; }
RESOURCE TBUF r_gadolinium_general { buf="Name: gadolinium\nSymbol: Gd\nAtomic number: 64\nCategory: lanthanides\nGroup: n/a\nPeriod: 6\nBlock: f\nAtomic weight: 157.25 g/mol\nElectrons per shell: 2, 8, 18, 25, 9, 2\nCAS number: 7440-54-2"; }
RESOURCE TBUF r_gadolinium_physical { buf="Phase: solid\nDensity: 7.90 kg/l (near room temperature)\nMelting point: 1585 K (1312°C, 2394°F)\nBoiling point: 3546 K (3273°C, 5923°F)"; }
RESOURCE TBUF r_gadolinium_atomic { buf="Crystal structure: hexagonal\nOxidation states: 3\nElectronegativity: 1.20 (Pauling scale)"; }
RESOURCE TBUF r_gadolinium_name { buf="Comes from: Johan Gadolin\nMeaning: the Finnish chemist and geologist"; }
RESOURCE TBUF r_gadolinium_other { buf="- is a silvery-white, malleable and ductile rare-\n  earth metal with a metallic lustre\n- crystallizes in hexagonal, close-packed alpha\n  form at room temperature, but, when heated to\n  1508 K or more, it transforms into its beta form,\n  which has a body-centered cubic structure"; }
RESOURCE TBUF r_gadolinium_other2 { buf="- unlike other rare earth elements, gadolinium is\n  relatively stable in dry air - however, it\n  tarnishes quickly in moist air, forming a\n  loosely-adhering oxide which spalls off, exposing\n  more surface to oxidation"; }
RESOURCE TBUF r_gadolinium_other3 { buf="- is used for making gadolinium yttrium garnets,\n  which have microwave applications, and gadolinium\n  compounds are used for making phosphors for\n  colour TV tubes\n- is used in nuclear marine propulsion systems as a\n  burnable poison"; }
RESOURCE TBUF r_gadolinium_other4 { buf="- has no known native biological role, but in\n  research on biological systems it has a few\n  roles - it is used as a component of MRI\n  contrast agents\n- is never found in nature as the free element, but\n  is contained in many rare minerals such as\n  monazite and bastnäsite"; }
RESOURCE TBUF r_gadolinium_other5 { buf="- occurs only in trace amounts in the mineral\n  gadolinite, which was also named after Johan\n  Gadolin\n- as with the other lanthanides, gadolinium\n  compounds are of low to moderate toxicity,\n  although their toxicity has not been investigated\n  in detail"; }
RESOURCE TBUF r_gadolinium_other6 { buf="- in patients with renal failure or other pro-\n  inflammatory conditions, there is data\n  associating its use with development of\n  nephrogenic fibrosing dermopathy as a side effect\n  of gadolinium chelates used as a contrast agent\n  for MRI examinations"; }
RESOURCE TBUF r_terbium_general { buf="Name: terbium\nSymbol: Tb\nAtomic number: 65\nCategory: lanthanides\nGroup: n/a\nPeriod: 6\nBlock: f\nAtomic weight: 158.92535 g/mol\nElectrons per shell: 2, 8, 18, 27, 8, 2\nCAS number: 7440-27-9"; }
RESOURCE TBUF r_terbium_physical { buf="Phase: solid\nDensity: 8.23 kg/l (near room temperature)\nMelting point: 1629 K (1356°C, 2473°F)\nBoiling point: 3503 K (3230°C, 5846°F)"; }
RESOURCE TBUF r_terbium_atomic { buf="Crystal structure: hexagonal\nOxidation states: 3, 4\nElectronegativity: 1.2 (Pauling scale)"; }
RESOURCE TBUF r_terbium_name { buf="Comes from: terbia\nMeaning: when the mineral yttria was partitioned\n         into three fractions, the 'terbia' was\n         the fraction that contained the pink color\n         (due to what now is known as erbium)"; }
RESOURCE TBUF r_terbium_other { buf="- is a silvery-white rare earth metal that is\n  malleable, ductile and soft enough to be cut with\n  a knife\n- is reasonably stable in air (it does not tarnish\n  after nineteen months at room temperature)\n- two crystal allotropes exist, with a\n  transformation temperature of 1289 C"; }
RESOURCE TBUF r_terbium_other2 { buf="- is used to dope calcium fluoride, calcium\n  tungstate and strontium molybdate, materials\n  that are used in solid-state devices, and as a\n  crystal stabilizer of fuel cells which operate at\n  elevated temperatures\n- is also used in alloys and in the production of\n  electronic devices"; }
RESOURCE TBUF r_terbium_other3 { buf="- is never found in nature as a free element, but\n  it is contained in many minerals, including\n  cerite, gadolinite, monazite\n- the richest current commercial sources of terbium\n  are the ion-adsorption clays of southern China"; }
RESOURCE TBUF r_terbium_other4 { buf="- can be produced by reducing the anhydrous\n  chloride or fluoride with calcium metal in a\n  tantalum crucible - calcium and tantalum\n  impurities can be removed by vacuum remelting"; }
RESOURCE TBUF r_terbium_other5 { buf="- as with the other lanthanides, terbium compounds\n  are of low to moderate toxicity, although their\n  toxicity has not been investigated in detail"; }
RESOURCE TBUF r_dysprosium_general { buf="Name: dysprosium\nSymbol: Dy\nAtomic number: 66\nCategory: lanthanides\nGroup: n/a\nPeriod: 6\nBlock: f\nAtomic weight: 162.500 g/mol\nElectrons per shell: 2, 8, 18, 28, 8, 2\nCAS number: 7429-91-6"; }
RESOURCE TBUF r_dysprosium_physical { buf="Phase: solid\nDensity: 8.540 kg/l (near room temperature)\nMelting point: 1680 K (1407°C, 2565°F)\nBoiling point: 2840 K (2562°C, 4653°F)"; }
RESOURCE TBUF r_dysprosium_atomic { buf="Crystal structure: hexagonal\nOxidation states: 3\nElectronegativity: 1.22 (Pauling scale)"; }
RESOURCE TBUF r_dysprosium_name { buf="Comes from: dysprositos\nMeaning: a Greek word which means 'hard to get' -\n         the discoverer tried to isolate the\n         element more than 30 times"; }
RESOURCE TBUF r_dysprosium_other { buf="- is a rare earth element with a metallic silver\n  luster\n- is never found in nature as a free element,\n  though it is found in various minerals, such as\n  xenotime\n- is relatively stable in air at room temperature,\n  but it dissolves readily in mineral acids"; }
RESOURCE TBUF r_dysprosium_other2 { buf="- is used for its high thermal neutron absorption\n  cross-section in making control rods in nuclear\n  reactors, for its high magnetic susceptibility to\n  magnetization in data storage devices and as a\n  component of Terfenol-D (a metal alloy)"; }
RESOURCE TBUF r_dysprosium_other3 { buf="- soluble dysprosium salts are mildly toxic, while\n  the insoluble salts are considered non-toxic\n- has no known biological role\n- most dysprosium is being obtained from the ion-\n  adsorption clay ores of southern China"; }
RESOURCE TBUF r_dysprosium_other4 { buf="- is used, in conjunction with vanadium and other\n  elements, in making laser materials\n- Dy powder may present an explosion hazard when\n  mixed with air and when an ignition source is\n  present"; }
RESOURCE TBUF r_dysprosium_other5 { buf="- dysprosium fires cannot be put out by water - it\n  can react with water to produce flammable\n  hydrogen gas"; }
RESOURCE TBUF r_holmium_general { buf="Name: holmium\nSymbol: Ho\nAtomic number: 67\nCategory: lanthanides\nGroup: n/a\nPeriod: 6\nBlock: f\nAtomic weight: 164.93032 g/mol\nElectrons per shell: 2, 8, 18, 29, 8, 2\nCAS number: 7440-60-0"; }
RESOURCE TBUF r_holmium_physical { buf="Phase: solid\nDensity: 8.79 kg/l (near room temperature)\nMelting point: 1734 K (1461°C, 2662°F)\nBoiling point: 2993 K (2720°C, 4928°F)"; }
RESOURCE TBUF r_holmium_atomic { buf="Crystal structure: hexagonal\nOxidation states: 3\nElectronegativity: 1.23 (Pauling scale)"; }
RESOURCE TBUF r_holmium_name { buf="Comes from: Holmia\nMeaning: the Latin name of Stockholm - the\n         discoverer's home town"; }
RESOURCE TBUF r_holmium_other { buf="- is a relatively soft and malleable silvery-white\n  metallic element\n- is stable in dry air at room temperature\n- is found in the minerals monazite and gadolinite,\n  not as a free element\n- quickly oxidizes in moist air, forming a\n  yellowish oxide"; }
RESOURCE TBUF r_holmium_other2 { buf="- is commercially extracted via ion-exchange from\n  monazite sand (0.05% holmium) but is still\n  difficult to separate from other rare earths\n- its estimated abundance in the Earth's crust is\n  1.3 mg/kg\n- plays no biological role in humans but may be\n  able to stimulate metabolism"; }
RESOURCE TBUF r_holmium_other3 { buf="- as with other rare earths, appears to have a low\n  degree of acute toxicity\n- because of its magnetic properties, holmium has\n  been used to create the strongest artificially-\n  generated magnetic fields when placed within\n  high-strength magnets as a magnetic pole piece"; }
RESOURCE TBUF r_holmium_other4 { buf="- ince it can absorb nuclear fission-bred neutrons,\n  the element is also used in nuclear control\n  rods\n- is also used as a yellow or red glass coloring"; }
RESOURCE TBUF r_erbium_general { buf="Name: erbium\nSymbol: Er\nAtomic number: 68\nCategory: lanthanides\nGroup: n/a\nPeriod: 6\nBlock: f\nAtomic weight: 167.259 g/mol\nElectrons per shell: 2, 8, 18, 30, 8, 2\nCAS number: 7440-52-0"; }
RESOURCE TBUF r_erbium_physical { buf="Phase: solid\nDensity: 9.066 kg/l (near room temperature)\nMelting point: 1802 K (1529°C, 2784°F)\nBoiling point: 3141 K (2868°C, 5194°F)"; }
RESOURCE TBUF r_erbium_atomic { buf="Crystal structure: hexagonal\nOxidation states: 3\nElectronegativity: 1.24 (Pauling scale)"; }
RESOURCE TBUF r_erbium_name { buf="Comes from: Ytterby\nMeaning: the village where the element was found"; }
RESOURCE TBUF r_erbium_other { buf="- is a rare, silvery, white metallic lanthanide\n  which is solid in its normal state\n- is malleable (or easily shaped), soft yet stable\n  in air, and does not oxidize as quickly as some\n  other rare-earth metals"; }
RESOURCE TBUF r_erbium_other2 { buf="- its salts are rose-colored, and the element has\n  characteristic sharp absorption spectra bands in\n  visible light, ultraviolet, and near infrared\n- does not play any known biological role, but is\n  thought by some to be able to stimulate\n  metabolism"; }
RESOURCE TBUF r_erbium_other3 { buf="- is commonly used as a photographic filter, and\n  because of its resilience it is useful as a\n  metallurgical additive\n- its estimated abundance in the Earth's crust is\n  1.3 mg/kg\n- plays no biological role in humans but may be\n  able to stimulate metabolism"; }
RESOURCE TBUF r_erbium_other4 { buf="- the mineral gadolinite was spearated into three\n  fractions which was called yttria, erbia, and\n  terbia - erbia and terbia, however, were confused\n  at this time, and after 1860, terbia was renamed\n  erbia and after 1877 what had been known as erbia\n  was renamed terbia"; }
RESOURCE TBUF r_erbium_other5 { buf="- is never found as a free element in nature but is\n  found bound in monazite sand ores\n- has historically been very difficult and\n  expensive to separate rare earths from each other\n  in their ores but ion-exchange production"; }
RESOURCE TBUF r_erbium_other6 { buf="  techniques developed in the late 20th century\n  have greatly brought down the cost of production\n  of all rare-earth metals and their chemical\n  compounds\n- metallic erbium in dust form presents a fire and\n  explosion hazard"; }
RESOURCE TBUF r_thulium_general { buf="Name: thulium \nSymbol: Tm\nAtomic number: 69\nCategory: lanthanides\nGroup: n/a\nPeriod: 6\nBlock: f\nAtomic weight: 168.93421 g/mol\nElectrons per shell: 2, 8, 18, 31, 8, 2\nCAS number: 7440-30-4"; }
RESOURCE TBUF r_thulium_physical { buf="Phase: solid\nDensity: 9.32 kg/l (near room temperature)\nMelting point: 1818 K (1545°C, 2813°F)\nBoiling point: 2223 K (1950°C, 3542°F)"; }
RESOURCE TBUF r_thulium_atomic { buf="Crystal structure: hexagonal\nOxidation states: 3\nElectronegativity: 1.25 (Pauling scale)"; }
RESOURCE TBUF r_thulium_name { buf="Comes from: Thule\nMeaning: the name of Scandinavia in classical\n         literature - the discoverer was Swedish"; }
RESOURCE TBUF r_thulium_other { buf="- is the least abundant of the rare earths\n- is an easily workable metal with a bright\n  silvery-gray luster and can be cut by a knife\n- has some corrosion resistance in dry air and good\n  ductility"; }
RESOURCE TBUF r_thulium_other2 { buf="- naturally occurring thulium is made entirely of\n  the stable isotope thulium-169\n- as a rare and expensive, thulium does not have\n  many practical applications"; }
RESOURCE TBUF r_thulium_other3 { buf="- has been used to create laser lights, but\n  production costs have been too high for\n  commercial use\n- is never found in nature in pure form, but it is\n  found in small quantities in minerals with other\n  rare earths"; }
RESOURCE TBUF r_thulium_other4 { buf="- is used as interstellar money in the book Illegal\n  Aliens, due to its rarity and lack of other\n  uses\n- has a low-to-moderate degree of acute toxicity\n  and should be handled with care, and metallic"; }
RESOURCE TBUF r_thulium_other5 { buf="  thulium in dust form presents a fire and\n  explosion hazard"; }
RESOURCE TBUF r_ytterbium_general { buf="Name: ytterbium \nSymbol: Yb\nAtomic number: 70\nCategory: lanthanides\nGroup: n/a\nPeriod: 6\nBlock: f\nAtomic weight: 173.04 g/mol\nElectrons per shell: 2, 8, 18, 32, 8, 2\nCAS number: 7440-64-4"; }
RESOURCE TBUF r_ytterbium_physical { buf="Phase: solid\nDensity: 6.90 kg/l (near room temperature)\nMelting point: 1097 K (824°C, 1515°F)\nBoiling point: 1469 K (1196°C, 2185°F)"; }
RESOURCE TBUF r_ytterbium_atomic { buf="Crystal structure: cubic face centered\nOxidation states: 2, 3\nElectronegativity: 1.1 (Pauling scale)"; }
RESOURCE TBUF r_ytterbium_name { buf="Comes from: Ytterby\nMeaning: the name of a village in Sweden"; }
RESOURCE TBUF r_ytterbium_other { buf="- is a soft, malleable and rather ductile element\n  that exhibits a bright silvery luster\n- is a rare earth of the lanthanide series and is\n  found in the minerals gadolinite, monazite, and\n  xenotime"; }
RESOURCE TBUF r_ytterbium_other2 { buf="- is sometimes associated with yttrium or other\n  related elements and is used in certain steels\n- natural ytterbium is a mix of seven stable\n  isotopes\n- as a rare and expensive, thulium does not have\n  many practical applications"; }
RESOURCE TBUF r_ytterbium_other3 { buf="- is fairly stable, it nevertheless should be\n  stored in closed containers to protect it from\n  air and moisture\n- compounds of ytterbium should be treated as\n  highly toxic although initial studies appear to"; }
RESOURCE TBUF r_ytterbium_other4 { buf="  indicate that the danger is limited\n- is used e.g. as a source of X-rays, in doping of\n  stainless steel, and in solar cells\n- metallic ytterbium dust poses a fire and\n  explosion hazard"; }
RESOURCE TBUF r_lutetium_general { buf="Name: lutetium\nSymbol: Lu\nAtomic number: 71\nCategory: lanthanides\nGroup: n/a\nPeriod: 6\nBlock: f\nAtomic weight: 174.967 g/mol\nElectrons per shell: 2, 8, 18, 32, 9, 2\nCAS number: 7439-94-3"; }
RESOURCE TBUF r_lutetium_physical { buf="Phase: solid\nDensity: 9.841 kg/l (near room temperature)\nMelting point: 1925 K (1652°C, 3006°F)\nBoiling point: 3675 K (3402°C, 6156°F)"; }
RESOURCE TBUF r_lutetium_atomic { buf="Crystal structure: hexagonal\nOxidation states: 3\nElectronegativity: 1.27 (Pauling scale)"; }
RESOURCE TBUF r_lutetium_name { buf="Comes from: Lutetia\nMeaning: the Latin name of Paris; the discoverer\n         was French"; }
RESOURCE TBUF r_lutetium_other { buf="- is a silvery white corrosion-resistant trivalent\n  metal that is relatively stable in air\n- is the heaviest and hardest of the rare earth\n  elements\n- has the highest melting point of any lanthanide,\n  probably related to the lanthanide contraction"; }
RESOURCE TBUF r_lutetium_other2 { buf="- is very expensive to obtain in useful quantities\n  and therefore it has very few commercial uses\n- stable lutetium can be used as catalysts in\n  petroleum cracking in refineries and can also be\n  used in alkylation, hydrogenation, and\n  polymerization applications"; }
RESOURCE TBUF r_lutetium_other3 { buf="- lutetium-176 has been used to date the age of\n  meteorites\n- was proposed with names aldebaranium,\n  neoytterbium, cassiopium and lutecium\n- found with almost all other rare-earth metals but\n  never by itself, lutetium is very difficult to\n  separate from other elements"; }
RESOURCE TBUF r_lutetium_other4 { buf="- is one of the most expensive metals, costing\n  about six times as much as gold\n- is regarded as having a low degree of toxicity\n  but it and especially its compounds should be\n  handled with care nonetheless"; }
RESOURCE TBUF r_lutetium_other5 { buf="- metal dust of this element is a fire and\n  explosion hazard\n- plays no biological role in the human body"; }
