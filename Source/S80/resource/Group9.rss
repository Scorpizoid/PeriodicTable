RESOURCE TBUF r_cobalt_physical { buf="Phase: solid\nDensity: 8.90 kg/l (near room temperature)\nMelting point: 1768 K (1495°C, 2723°F)\nBoiling point: 3200 K (2927°C, 5301°F)"; }
