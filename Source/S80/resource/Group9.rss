RESOURCE TBUF r_iridium_general { buf="Name: iridium\nSymbol: Ir\nAtomic number: 77\nCategory: transition metals\nGroup: 9\nPeriod: 6\nBlock: d\nAtomic weight: 192.217 g/mol\nElectrons per shell: 2, 8, 18, 32, 15, 2\nCAS number: 7439-88-5"; }
RESOURCE TBUF r_iridium_physical { buf="Phase: solid\nDensity: 22.56 kg/l (near room temperature)\nMelting point: 2739 K (2466°C, 4471°F)\nBoiling point: 4701 K (4428°C, 8002°F)"; }
RESOURCE TBUF r_iridium_atomic { buf="Crystal structure: cubic face centered\nOxidation states: -3,-1, 0, 1, 2, 3, 4, 5, 6\nElectronegativity: 2.20 (Pauling scale)"; }
RESOURCE TBUF r_iridium_name { buf="Comes from: Iris\nMeaning: the Greek winged goddess of the rainbow\n       and the messenger of the Olympian gods,\n       because many of the salts he obtained\n         were strongly colored"; }
RESOURCE TBUF r_iridium_other { buf="- a very hard, brittle, silvery-white transition\n  metal of the platinum family\n- is the second densest element and is the most\n  corrosion- resistant metal, even at temperatures\n  as high as 2000 C\n- is one of the rarest elements in the Earth's\n  crust"; }
RESOURCE TBUF r_iridium_other2 { buf="- annual production and consumption is only three\n  tonnes\n- iridium compounds also find applications as\n  catalysts for the production of acetic acid"; }
RESOURCE TBUF r_iridium_other3 { buf="- is used in spark plugs, crucibles for\n  recrystallization of semiconductors at high\n  temperatures, electrodes for the production of\n  chlorine in the chloralkali process, and\n  radioisotope thermoelectric generators used in\n  unmanned spacecraft"; }
