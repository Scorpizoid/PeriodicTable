RESOURCE TBUF r_dubnium_general { buf="Name: dubnium\nSymbol: Db\nAtomic number: 105\nCategory: transition metals\nGroup: 5\nPeriod: 7\nBlock: d\nAtomic weight: [268] g/mol\nElectrons per shell: 2, 8, 18, 32, 32, 11, 2\nCAS number: 53850-35-4"; }
