RESOURCE TBUF r_vanadium_physical { buf="Phase: solid\nDensity: 6.0 kg/l (near room temperature)\nMelting point: 2183 K (1910°C, 3470°F)\nBoiling point: 3680 K (3407°C, 6165°F)"; }
