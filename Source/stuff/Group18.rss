/* Group 18 */
RESOURCE TBUF r_helium_general { buf="Name: helium\nSymbol: He\nAtomic number: 2\nCategory: noble gases\nGroup: 18\nPeriod: 1\nBlock: s\nAtomic weight: 4.002602 g/mol\nElectrons per shell: 2\nCAS number: 7440-59-7"; }
RESOURCE TBUF r_helium_physical { buf="Phase: gas\nDensity: 0.1786 g/l (0 C, 101.325 kPa)\nMelting point: 0.95 K (-272.20 C, -457.96 F)\n             (at 2.5 MPa)\nBoiling point: 4.22 K (-268.93 C, -452.07 F)"; }
RESOURCE TBUF r_helium_atomic { buf="Crystal structure: hexagonal close-packed\nOxidation states: 0\nElectronegativity: unknown"; }
RESOURCE TBUF r_helium_name { buf="Comes from: Helios\nMeaning: the Greek name for Sun because the\n         discoverer concluded this element is found\n         in the Sun"; }
RESOURCE TBUF r_helium_other { buf="- is a colorless, odorless, tasteless, non-toxic,\n  inert monatomic gas\n- its boiling and melting points are the lowest\n  among the elements and it exists only as a gas\n  except in extreme conditions"; }
RESOURCE TBUF r_helium_other2 { buf="- is used in cryogenics, in deep-sea breathing\n  systems, to cool superconducting magnets, in\n  helium dating, for inflating balloons, for\n  providing lift in airships and as a protective\n  gas for many industrial uses (such as arc\n  welding and growing silicon wafers)"; }
RESOURCE TBUF r_helium_other3 { buf="- haling a small volume of the gas temporarily\n  changes the timbre and quality of the human\n  voice\n- is the second lightest element and is the second\n  most abundant in the observable Universe"; }
RESOURCE TBUF r_helium_other4 { buf="- most helium was formed during the Big Bang, but\n  new helium is being created as a result of the\n  nuclear fusion of hydrogen in stars\n- has two liquid states - helium I and helium II -\n  helium II is a superfluid which has the same\n  viscosity as the gaseuous helium"; }
RESOURCE TBUF r_helium_other5 { buf="- at standard conditions is non-toxic, plays no\n  biological role and is found in trace amounts in\n  human blood - if enough helium is inhaled that\n  oxygen needed for normal respiration is replaced\n  asphyxia is possible"; }
RESOURCE TBUF r_neon_general { buf="Name: neon\nSymbol: Ne\nAtomic number: 10\nCategory: noble gases\nGroup: 18\nPeriod: 2\nBlock: p\nAtomic weight: 20.1797 g/mol\nElectrons per shell: 2, 8\nCAS number: 7440-01-9"; }
RESOURCE TBUF r_neon_physical { buf="Phase: gas\nDensity: 0.9002 g/l (0 C, 101.325 kPa)\nMelting point: 24.56 K (-248.59 C, -415.46 F)\nBoiling point: 27.07 K (-246.08 C, -410.94 F)"; }
RESOURCE TBUF r_neon_atomic { buf="Crystal structure: cubic face centered\nOxidation states: unknown\nElectronegativity: inert"; }
RESOURCE TBUF r_neon_name { buf="Comes from: neon\nMeaning: a Greek word which means 'new one'"; }
RESOURCE TBUF r_neon_other { buf="- a very common element in the universe but rare on\n  Earth\n- gives a distinct reddish-orange glow when used in\n  discharge tubes and neon lamps\n- is commercially extracted from air, in which it\n  is found in trace amounts\n- is the second-lightest noble gas"; }
RESOURCE TBUF r_neon_other2 { buf="- has over 40 times the refrigerating capacity of\n  liquid helium\n  and three times that of liquid hydrogen (on a per\n  unit volume basis)\n- is the fifth most abundant chemical element in\n  the universe by mass, after hydrogen, helium,\n  oxygen, and carbon"; }
RESOURCE TBUF r_neon_other3 { buf="- is used in vacuum tubes, high-voltage indicators,\n  lightning arrestors, wave meter tubes, television\n  tubes, light signs, and helium-neon lasers"; }
RESOURCE TBUF r_neon_other4 { buf="- liquefied neon is commercially used as a\n  cryogenic refrigerant in applications not\n  requiring the lower temperature range attainable\n  with more extreme liquid helium refrigeration"; }
RESOURCE TBUF r_argon_general { buf="Name: argon\nSymbol: Ar\nAtomic number: 18\nCategory: noble gases\nGroup: 18\nPeriod: 3\nBlock: p\nAtomic weight: 39.948 g/mol\nElectrons per shell: 2, 8, 8\nCAS number: 7440–37–1"; }
RESOURCE TBUF r_argon_physical { buf="Phase: gas\nDensity: 1.784 g/l (0 C, 101.325 kPa)\nMelting point: 83.80 K (-189.35 C, -308.83 F)\nBoiling point: 87.30 K (-185.85 C, -302.53 F)"; }
RESOURCE TBUF r_argon_atomic { buf="Crystal structure: cubic face centered\nOxidation states: 0\nElectronegativity: inert"; }
RESOURCE TBUF r_argon_name { buf="Comes from: argon\nMeaning: a Greek word which means 'inactive' due\n         to the element's inactivity"; }
RESOURCE TBUF r_argon_other { buf="- is present in the Earth's atmosphere at 0.93% by\n  volume\n- is the most abundant and most frequently used of\n  the noble gases\n- is colorless, odorless, tasteless and nontoxic in\n  both its liquid and gaseous forms\n- up until 1957 its symbol was only A"; }
RESOURCE TBUF r_argon_other2 { buf="- argon was thought to be inert and unable to form\n  any compounds, but in August 2000 the first argon\n  compound was formed by the researchers at the\n  University of Helsinki in Finland - by shining\n  ultraviolet light onto frozen argon containing a\n  small amount of hydrogen fluoride, argon\n  fluorohydride (HArF) was formed"; }
RESOURCE TBUF r_argon_other3 { buf="- is used e.g as a fire extinguisher gas, metal\n  inert gas welding, as a preservative and in\n  laboratory gloveboxes\n- cryosurgery procedures such as cryoablation use\n  liquefied argon to destroy cancer cells"; }
RESOURCE TBUF r_argon_other4 { buf="- although argon is non-toxic, it does not satisfy\n  the body's need for oxygen and is thus an\n  asphyxiant"; }
RESOURCE TBUF r_krypton_general { buf="Name: krypton\nSymbol: Kr\nAtomic number: 36\nCategory: noble gases\nGroup: 18\nPeriod: 4\nBlock: p\nAtomic weight: 83.798 g/mol\nElectrons per shell: 2, 8, 18, 8\nCAS number: 7439-90-9"; }
RESOURCE TBUF r_krypton_physical { buf="Phase: gas\nDensity: 3.7494 g/l (0 C, 101.325 kPa)\nMelting point: 115.79 K (-157.36 C, -251.25 F)\nBoiling point: 119.93 K (-153.22 C, -244.12 F)"; }
RESOURCE TBUF r_krypton_atomic { buf="Crystal structure: cubic face centered\nOxidation states: 4, 2\nElectronegativity: 3.00 (Pauling scale)"; }
RESOURCE TBUF r_krypton_name { buf="Comes from: kryptos\nMeaning: a Greek word which means 'hidden' because\n         it's found in trace amounts in the Earth's\n         atmosphere"; }
RESOURCE TBUF r_krypton_other { buf="- a colorless, odorless and tasteless noble gas\n- is isolated by fractionally distilling liquified\n  air\n- is often used with other rare gases in\n  fluorescent lamps\n- has a brilliant green and orange spectral\n  signature"; }
RESOURCE TBUF r_krypton_other2 { buf="- its concentration in the atmosphere is about 1\n  ppm\n- little is known about whether krypton is\n  dangerous, but it is treated as a non-toxic\n  asphyxiant\n- costs 100 times as much as argon"; }
RESOURCE TBUF r_xenon_general { buf="Name: xenon\nSymbol: Xe\nAtomic number: 54\nCategory: noble gases\nGroup: 18\nPeriod: 5\nBlock: p\nAtomic weight: 131.293 g/mol\nElectrons per shell: 2, 8, 18, 18, 8\nCAS number: 7440-63-3"; }
RESOURCE TBUF r_xenon_physical { buf="Phase: gas\nDensity: 5.894 g/l (0 C, 101.325 kPa)\nMelting point: (101.325 kPa) 161.4 K (-111.7 C,\n              -169.1 F)\nBoiling point: (101.325 kPa) 165.03 K (-108.12 C,\n              -162.62 F)"; }
RESOURCE TBUF r_xenon_atomic { buf="Crystal structure: cubic face centered\nOxidation states: 0, 1, 2, 4, 6, 8\nElectronegativity: 2.6 (Pauling scale)"; }
RESOURCE TBUF r_xenon_name { buf="Comes from: xenos\nMeaning: a Greek word which means 'stranger'\n         because it's found so little in the\n         Earth's atmosphere"; }
RESOURCE TBUF r_xenon_other { buf="- occurs in the Earth's atmosphere in trace amounts\n  (0.087 ppm)\n- although generally unreactive, xenon can undergo\n  a few chemical reactions such as the formation of\n  xenon hexafluoroplatinate, the first noble gas\n  compound to be synthesized"; }
RESOURCE TBUF r_xenon_other2 { buf="- is used in flash lamps and arc lamps, and as a\n  general anesthetic\n- is also being used to search for hypothetical\n  weakly interacting massive particles and as the\n  propellant for ion thrusters in spacecraft"; }
RESOURCE TBUF r_xenon_other3 { buf="- can be safely kept in normal sealed glass or\n  metal containers at standard temperature and\n  pressure, although it readily dissolves in most\n  plastics and rubber, and will gradually escape\n  from a container sealed with such material"; }
RESOURCE TBUF r__other4 { buf="- naturally occurring xenon consists of nine stable\n  isotopes, and there are also over 40 unstable\n  isotopes that undergo radioactive decay"; }
RESOURCE TBUF r_radon_general { buf="Name: radon\nSymbol: Ra\nAtomic number: 86\nCategory: noble gases\nGroup: 18\nPeriod: 6\nBlock: p\nAtomic weight: (222) g/mol\nElectrons per shell: 2, 8, 18, 32, 18, 8\nCAS number: 10043-92-2"; }
RESOURCE TBUF r_radon_physical { buf="Phase: gas\nDensity: 9.73 g/l (0 C, 101.325 kPa)\nMelting point: 202 K (-71.15 C, -96 F)\nBoiling point: 211.3 K (-61.85 C, -79.1 F)"; }
RESOURCE TBUF r_radon_atomic { buf="Crystal structure: cubic face centered\nOxidation states: 0\nElectronegativity: 2.2 (Pauling scale)"; }
RESOURCE TBUF r_radon_name { buf="Comes from: radium emanation\nMeaning: a shortened version of the phrase -\n         emanation is from the Latin word\n        'emanatio' (expiration)"; }
RESOURCE TBUF r_radon_other { buf="- is a colorless, odorless, tasteless, naturally\n  occurring, radioactive noble gas that is formed\n  from the decay of radium\n- is one of the heaviest substances that remains a\n  gas under normal conditions and is considered to\n  be a health hazard"; }
RESOURCE TBUF r_radon_other2 { buf="- the most stable isotope, radon-222, has a half-\n  life of 3.8 days\n- will continue to exist indefinitely at about the\n  same levels as it is now"; }
RESOURCE TBUF r_radon_other3 { buf="- is responsible for the majority of the mean\n  public exposition to ionizing radiations, it is\n  often the single largest contributor to an\n  individual's background radiation dose"; }
RESOURCE TBUF r_radon_other4 { buf="- is reportedly the second most frequent cause of\n  lung cancer, after cigarette smoking; and radon-\n  induced lung cancer the 6th leading cause of\n  cancer death overall"; }
RESOURCE TBUF r_radon_other5 { buf="- the radon itself isn't very dangerous, but its\n  radioactive decay products are - polonium-218\n  and 214 - which remain lodged in the lungs, and\n  continue to decay, causing continued exposure by\n  emitting alpha radiations"; }
