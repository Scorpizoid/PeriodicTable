RESOURCE TBUF r_platinum_physical { buf="Phase: solid\nDensity: 21.45 kg/l (near room temperature)\nMelting point: 2041.4 K (1768.3°C, 3214.9°F)\nBoiling point: 4098 K (3825°C, 6917°F)"; }
